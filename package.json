{
  "name": "quiz-api",
  "version": "1.0.0",
  "description": "REST API for multiplication quiz sessions",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "start": "node dist/server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch 'src/server/**/*.ts' --exec 'ts-node' src/server/index.ts",
    "dev:client": "vite",
    "test": "jest",
    "playwright": "playwright test",
    "playwright:ui": "playwright test --ui"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@playwright/test": "^1.57.0",
    "@types/bcrypt": "^6.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/pg": "^8.16.0",
    "@vercel/postgres": "^0.10.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.1",
    "dat.gui": "^0.7.9",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-list-endpoints": "^7.1.1",
    "jest": "^30.2.0",
    "jsonwebtoken": "^9.0.3",
    "lil-gui": "^0.21.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "stats-gl": "^3.8.0",
    "three": "^0.159.0",
    "three-stdlib": "^2.36.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/dat.gui": "^0.7.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.5.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@types/three": "^0.182.0",
    "@types/uuid": "^11.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^7.3.1"
  }
}
