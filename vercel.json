{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/auth",
      "destination": "/api/index.ts"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/session",
      "destination": "/api/index.ts"
    },
    {
      "source": "/session/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/credit",
      "destination": "/api/index.ts"
    },
    {
      "source": "/credit/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/endpoints",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
